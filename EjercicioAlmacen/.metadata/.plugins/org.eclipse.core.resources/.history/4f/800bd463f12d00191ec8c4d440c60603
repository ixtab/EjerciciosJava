import taller.Motor;
import taller.Coche;
public class Inicio {
	public static void main(String[] args) {
		
		System.out.println(Coche.info);
		
		Motor miMotor = new Motor(100, 1);
		Coche miCoche = new Coche("5568FJK","Ford","Fiesta", miMotor); // Composicion débil.
		System.out.println(miCoche.estado());
		
		System.out.println(miCoche.getMotor().estado());
		
		miCoche = null; 
		System.out.println("El objeto motor sigue activo con caballos " 
			+ miMotor.getCaballos());
		
		Coche miCoche2 = new Coche("5522FGH","Citroen","C1", 150, 0); // Composión fuerte.
		System.out.println(miCoche2.estado());
		
		System.out.println(Coche.contadorCoches);
		
	}
}
