
public class Perro extends Animal {

	public Perro(String nombre) {
		super("Perro");
		
	}

	public Perro() {
		super("Perro");
	}

	@Override
	public String morder(Animal ani) {
		return "Perro muerde " + ani; 
	}

	@Override
	public String mover() {
		return "El perro corre por el campo";
	}

}
