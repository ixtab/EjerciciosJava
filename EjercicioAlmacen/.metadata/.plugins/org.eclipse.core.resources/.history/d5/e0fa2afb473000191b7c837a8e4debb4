
public class Coche implements Vehiculo {
	
	private int velocidad;
	
	public Coche() {
		
	}
	
	@Override
	public String frenar(int cuanto) {
		velocidad = velocidad - cuanto;
		return "El coche va a " + velocidad; 
	}

	@Override
	public String acelerar(int cuanto) {
		velocidad = velocidad + cuanto;
		return "El coche va a " + velocidad; 
	}

}
