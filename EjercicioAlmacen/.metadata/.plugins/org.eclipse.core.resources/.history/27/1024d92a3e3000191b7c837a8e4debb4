
public abstract class Animal {
	private String nombre;

	public Animal(String nombre) {
		this.nombre = nombre;
	}

	
	@Override
	public String toString() {
		return "Soy un " + nombre ;
	}

	// Setters y Getters
	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	
	
}
