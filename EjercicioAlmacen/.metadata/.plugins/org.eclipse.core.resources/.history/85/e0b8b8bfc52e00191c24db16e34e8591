
public class Inicio {

	public static void main(String[] args) {
		Persona p;
		p = new Alumno("As","Asd", 10354);
		System.out.println(p); // to String se implementa automáticamente.
		p = new Profesor("As","Asd", "Mates");
		System.out.println(p); // to String se implementa automáticamente.
	}

}
